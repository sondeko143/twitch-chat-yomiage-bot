    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "test",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "yomiage",
            "module": "uvicorn",
            "type": "python",
            "request": "launch",
            "args": [
                "yomiage.main:app",
                "--reload",
                "--port",
                "8000"
            ]
        },
    ]
